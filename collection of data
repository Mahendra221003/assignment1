#list
nums=[4,5,6,75,86,89] 
print(nums[0])
print(nums[-1])
nums.append("mahi")
print(nums)

#tuple
fruits=("apple","banana","grapes")
print(fruits.index("grapes"))
print(fruits.count("apple"))   

#set
myset={"data",100,True}
print(len(myset))
print(myset[0])

#dictionary
person={"name":"tomy","mobile":99}
person["mobile"]=33433
print(person)
